class Student {
    private String registerNumber;
    private String name;
    private int marks;

    // Constructor to initialize register number, name, and marks
    public Student(String registerNumber, String name, int marks) {
        this.registerNumber = registerNumber;
        this.name = name;
        this.marks = marks;
    }

    // Method to print student details
    public void printDetails() {
        System.out.println("Register Number: " + registerNumber);
        System.out.println("Name: " + name);
        System.out.println("Marks: " + marks);
    }
}

public class StudentDemo {
    public static void main(String[] args) {
        // Create a Student object with register number, name, and marks
        Student student = new Student("12345", "John Doe", 85);

        // Print the details of the student
        student.printDetails();
    }
}
